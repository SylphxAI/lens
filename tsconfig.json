{
	"extends": "@sylphx/tsconfig",
	"compilerOptions": {
		"allowImportingTsExtensions": true,
		"forceConsistentCasingInFileNames": true,
		"sourceMap": true,
		"composite": false,
		"noUncheckedIndexedAccess": false,
		"noPropertyAccessFromIndexSignature": false,
		"jsx": "react-jsx"
	},
	"exclude": [
		"examples",
		"**/*.test.ts",
		"**/*.test.tsx",
		"packages/preact",
		"packages/solid",
		"packages/solidstart"
	]
}
