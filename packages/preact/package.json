{
	"name": "@sylphx/lens-preact",
	"version": "2.0.6",
	"description": "Preact bindings for Lens API framework",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./signals": {
			"import": "./dist/signals.js",
			"types": "./dist/signals.d.ts"
		}
	},
	"scripts": {
		"build": "bun build ./src/index.ts ./src/signals.ts --outdir ./dist --target browser --external preact --external @preact/signals && bunx tsc --emitDeclarationOnly --declaration --outDir ./dist",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"prepack": "[ -d dist ] || bun run build"
	},
	"files": [
		"dist",
		"src"
	],
	"keywords": [
		"lens",
		"preact",
		"hooks",
		"reactive"
	],
	"author": "SylphxAI",
	"license": "MIT",
	"dependencies": {
		"@sylphx/lens-client": "workspace:*"
	},
	"peerDependencies": {
		"preact": ">=10.0.0",
		"@preact/signals": ">=1.0.0"
	},
	"peerDependenciesMeta": {
		"@preact/signals": {
			"optional": true
		}
	},
	"devDependencies": {
		"@preact/signals": "^2.0.0",
		"@testing-library/preact": "^3.2.4",
		"happy-dom": "^20.0.10",
		"preact": "^10.25.4",
		"typescript": "^5.9.3"
	}
}
