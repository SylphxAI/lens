{
	"name": "@sylphx/lens-solid",
	"version": "2.0.4",
	"description": "SolidJS bindings for Lens API framework",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		}
	},
	"scripts": {
		"build": "bun build ./src/index.ts --outdir ./dist --target browser --external solid-js && bunx tsc --emitDeclarationOnly --declaration --outDir ./dist",
		"typecheck": "tsc --noEmit",
		"test": "bun test",
		"prepack": "[ -d dist ] || bun run build"
	},
	"files": [
		"dist",
		"src"
	],
	"keywords": [
		"lens",
		"solid",
		"solidjs",
		"reactive",
		"signals"
	],
	"author": "SylphxAI",
	"license": "MIT",
	"dependencies": {
		"@sylphx/lens-client": "workspace:*"
	},
	"peerDependencies": {
		"solid-js": ">=1.8.0"
	},
	"devDependencies": {
		"@solidjs/testing-library": "^0.8.10",
		"solid-js": "^1.9.5",
		"typescript": "^5.9.3"
	}
}
