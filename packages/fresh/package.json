{
	"name": "@sylphx/lens-fresh",
	"version": "1.1.0",
	"description": "Fresh (Deno/Preact) integration for Lens API framework",
	"type": "module",
	"main": "./dist/index.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.js",
			"types": "./dist/index.d.ts"
		},
		"./islands": {
			"import": "./dist/islands.js",
			"types": "./dist/islands.d.ts"
		}
	},
	"scripts": {
		"build": "bun build ./src/index.ts ./src/islands.ts --outdir ./dist --target browser --external preact --external @preact/signals && tsc --emitDeclarationOnly --declaration --outDir ./dist",
		"typecheck": "tsc --noEmit",
		"test": "bun test"
	},
	"files": [
		"dist",
		"src"
	],
	"keywords": [
		"lens",
		"fresh",
		"deno",
		"preact",
		"islands"
	],
	"author": "SylphxAI",
	"license": "MIT",
	"dependencies": {
		"@sylphx/lens-client": "workspace:*",
		"@sylphx/lens-preact": "workspace:*",
		"@sylphx/lens-server": "workspace:*"
	},
	"peerDependencies": {
		"preact": ">=10.0.0",
		"@preact/signals": ">=1.0.0"
	},
	"peerDependenciesMeta": {
		"@preact/signals": {
			"optional": true
		}
	},
	"devDependencies": {
		"@preact/signals": "^2.0.0",
		"preact": "^10.25.4",
		"typescript": "^5.9.3"
	}
}
