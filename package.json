{
  "name": "lens",
  "version": "0.1.0",
  "private": true,
  "packageManager": "bun@1.3.3",
  "description": "Type-safe, real-time API framework",
  "type": "module",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "scripts": {
    "build": "turbo build",
    "test": "turbo test",
    "test:watch": "bun test --watch",
    "lint": "biome check .",
    "lint:fix": "biome check --fix .",
    "format": "biome format --write .",
    "typecheck": "turbo typecheck",
    "prepublishOnly": "test -n \"$CI\" || (echo 'Use CI for releases' && exit 1)"
  },
  "devDependencies": {
    "@biomejs/biome": "^2.3.8",
    "@sylphx/biome-config": "^0.4.0",
    "@sylphx/bump": "^1.3.7",
    "@sylphx/doctor": "^1.23.3",
    "@sylphx/tsconfig": "^0.3.0",
    "@types/bun": "latest",
    "@typescript/native-preview": "^7.0.0-dev.20251127.1",
    "bunup": "^0.16.10",
    "lefthook": "^2.0.4",
    "turbo": "^2.6.1",
    "typescript": "^5.9.3"
  },
  "keywords": [
    "typescript",
    "type-safe",
    "real-time",
    "api",
    "lens",
    "zod",
    "graphql",
    "trpc"
  ],
  "author": "SylphxAI",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/SylphxAI/Lens.git"
  },
  "dependencies": {
    "happy-dom": "^20.0.10"
  }
}
